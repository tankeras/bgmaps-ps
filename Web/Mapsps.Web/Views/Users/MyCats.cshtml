@using Mapsps.Services;
@model List<AllCatsViewModel>
@inject UserService userService

@{
    int petted = (int)(ViewData["Petted"]);
    int total = (int)(ViewData["Total"]);
    double percentage = (double)petted / (double)total * 100;
}  

<div class="progress"> 
    <div class="progress-bar" role="progressbar" style="width:@percentage%" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100"></div>
   
</div>

<h3 class="text-center">You have pet @petted/@total писис in @Model[0].Region</h3>
